{
  "serve": {
    "path": "serve.py",
    "script": "run",
    "parser": "get_parser",
    "help": "Run wptserve server",
    "virtualenv": true,
    "conditional_requirements": {
      "commandline_flag": {
        "webtransport_h3": [
          "../webtransport/requirements.txt"
        ],
        "dns": [
          "requirements_dns.txt"
        ]
      }
    }
  },
  "serve-wave": {
    "path": "wave.py",
    "script": "run",
    "parser": "get_parser",
    "help": "Run wptserve server for WAVE",
    "virtualenv": true,
    "requirements": ["../wave/requirements.txt"]
  }
}
